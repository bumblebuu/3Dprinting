extends layout

block content
  .container-fluid.carousel-container
    .row
      h1.ml-5.mt-3.mb-5.text-dark Some of our best sellers
    .row
      .col-10.offset-1
        #carouselExampleIndicators.carousel.slide.mb-5(data-ride='carousel')
          .carousel-inner
            .carousel-item.active
              .row.mb-3
                .col-4.offset-1.ml-5.mt-3.mb-5.grey-square
                  h1.text-dark.ml-3=productCarousel[0].name
                  ul.mt-3.text-dark
                    each desc in productCarousel[0].shortdescription.split('.') 
                      li.ml-5
                        h4=desc
                  a.btn.btn-lg.btn-dark.mt-3.ml-5(href=`/products/product/${productCarousel[0].seo}`) Show
                .col-7.square.mb-5
                    img.carousel-img.mb-5(src=`${productCarousel[0].img[0]}`)

            each product in productCarousel
              if product !== productCarousel[0]
                .carousel-item
                  .row.mb-3
                    .col-4.offset-1.ml-5.mt-3.mb-5.grey-square
                      h1.text-dark.ml-3=product.name
                      ul.text-dark
                        each desc in product.shortdescription.split('.') 
                          li.ml-5
                            h4= desc
                      a.btn.btn-lg.btn-dark.mt-3.ml-5(href=`/products/product/${product.seo}`) Show
                    .col-7.square.mb-5
                      img.carousel-img.mb-5(src=`${product.img[0]}`)
          a.carousel-control-prev.mr-5(href='#carouselExampleIndicators', role='button', data-slide='prev')
          a.carousel-control-prev.mr-5(href='#carouselExampleIndicators', role='button', data-slide='prev')
            span.carousel-control-prev-icon(aria-hidden='true')
            span.sr-only Previous
          a.carousel-control-next.ml-5(href='#carouselExampleIndicators', role='button', data-slide='next')
            span.carousel-control-next-icon(aria-hidden='true')
            span.sr-only Next
